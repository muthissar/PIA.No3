{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Flask",
            "type": "debugpy",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "${workspaceFolder}/html/app.py",
                "FLASK_DEBUG": "1"
            },
            "args": [
                "run",
                "--no-debugger",
                // "--no-reload",
                "--port=8000",
            ],
            // NOTE: jinja debugging is not supported for some reason?
            // "jinja": true,
            "jinja": false,
            "justMyCode": true,
            
        },
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8000",
            "webRoot": "${workspaceFolder}/html"
        },
        {
            "name": "combine_configs",
            "type": "debugpy",
            "request": "launch",
            "module": "combine_configs",
            "justMyCode": false,
            "args": [
                // "--configs",
                // "out/experiment-time-points-generator-fixedsteptimepoints-step-0-5-weight-movingaverage-window-size-0-5-c-0-5-dataset-datacache-label-validation-n-inpaint-128-n-pieces-100-ic-curve-none/k-traces-64/config.yaml",
                // "out/experiment-time-points-generator-fixedsteptimepoints-step-0-5-weight-movingaverage-window-size-0-5-c-0-5-dataset-datacache-label-validation-n-inpaint-128-n-pieces-100-ic-curve-none/k-traces-1/config.yaml",
            ]
        },
        {
            "name": "Python: plot",
            "type": "debugpy",
            "request": "launch",
            "module": "ic_gen",
            "justMyCode": false,
            "args": [
                "--config",
                // "configs/fixed_window_10s.yaml",
                // "configs/batik331_copy_ic_curve.yaml",
                "configs/batik.jsonnet",
                // "out/71a5597a6640d2bf7140e4ad1196901e5cf0085ac060e1a7cf337e20a248fb2f/samplingconfig-k-traces-128-temperature-1-0-top-p-0-9-top-k-0/config.yaml",
                "plot"
            ]
        },
        {
            "name": "Python: eval",
            "type": "debugpy",
            "request": "launch",
            "module": "ic_gen",
            "justMyCode": false,
            "args": [
                "--config",
                // "configs/fixed_window_10s.yaml",
                "configs/template_ics.yaml",
                "eval",
                "--out_file",
                "out/results/result_ics.h5",

            ],
            "env": {
                "CUDA_DEVICE_ORDER": "PCI_BUS_ID",
                "CUDA_VISIBLE_DEVICES": "3"
            }
        },
        {
            "name": "Python: Remote Attach",
            "type": "debugpy",
            "request": "attach",
            "connect": {
                "host": "127.0.0.1",
                "port": 5678
            },
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "."
                }
            ],
            "justMyCode": true
        },
        {
            "name": "Python: ic_gen",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/ic_gen.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--config",
                "configs/batik.jsonnet",
                // "configs/fixed_window_10s.yaml",
                // "configs/mozart_segment_2.yaml",
                // "configs/template_ics.yaml",
                "gen"
            ],

            "env": {
                "CUDA_DEVICE_ORDER": "PCI_BUS_ID",
                "CUDA_VISIBLE_DEVICES": "0"
            }
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "train",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--config=CIA/configs/piarceiverRw.py",
                "-t",
                "--num_workers=0"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "1",
            }
        },
        {
            "name": "generate",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--config=models/piarceiverRw_2022-02-02_09:36:13/config.py",
                "-o",
                "-l",
                "--num_workers=0"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "0",
            }
        }
    ]
}